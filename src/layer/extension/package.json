{
  "name": "live-lambda-extension",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist live-lambda-extension-exec extensions",
    "compile": "npx tsc",
    "package": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=node --target=node18 --format=cjs --external:aws-sdk --external:@aws-sdk/*",
    "build": "npm run clean && npm run package"
  },
  "bin": {
    "live-lambda-extension": "dist/index.js"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/credential-providers": "^3.810.0",
    "@aws-sdk/protocol-http": "^3.370.0",
    "@aws-sdk/signature-v4": "^3.370.0",
    "colors": "^1.4.0"
  },
  "devDependencies": {
    "@types/express": "^4",
    "@types/node": "^20",
    "esbuild": "^0.25.4",

    "typescript": "^5.0.0"
  }
}
